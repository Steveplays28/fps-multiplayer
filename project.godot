config_version=4

[application]

config/name="fps-multiplayer"
config/description="Unnamed multiplayer FPS game, made in Godot."
run/main_scene="res://common/scenes/main.tscn"
config/icon="res://icon.png"

[autoload]

ServerManager="*res://server/prefabs/autoload/server_manager.tscn"
ServerReferenceManager="*res://server/prefabs/autoload/server_reference_manager.tscn"

[debug]

multirun/number_of_windows=2
multirun/window_distance=700
multirun/add_custom_args=true
multirun/first_window_args=""
multirun/other_window_args="--client"

[display]

window/size/width=768
window/size/height=432

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg" )

